import java.io.File;

import javax.imageio.ImageIO;

class Tube {
	// Coordinates
	int tube_x;
	int tube_y;
	
	// Image stuff
	boolean facing_up;
	Image tube_image;
	
	Tube(int x, int y) {
		this.tube_x = x;
		this.tube_y = y;
		this.facing_up = true;
		
		if (facing_up) this.tube_image = ImageIO.read(new File("tubeup.png"));
		else this.tube_image = ImageIO.read(new File("tubedown.png"));
	}
}