abstract class Sprite {
	// Variables
	int x;
	int y;
	String image_name1;
	String image_name2;
	
	Sprite(int x, int y, String name1, String name2) {
		// Initialize variables
		this.x = x;
		this.y = y;
		this.image_name1 = name1;
		this.image_name2 = name2;
	}
	
	public abstract void update();
	
	public abstract void draw();
}